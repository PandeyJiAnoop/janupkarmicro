<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="3dp"
    android:layout_margin="3dp">
    <LinearLayout
        android:background="@drawable/rectangle_gradient_white"
        android:padding="10dp"
        android:id="@+id/live_ll"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:background="#FFE9E9"
                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textStyle="bold"
                    android:layout_width="100dp"
                    android:text="S.No."
                    android:gravity="center_vertical"

                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="Name"
                    android:gravity="center_vertical"

                    android:textStyle="bold"


                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="AOneStar Id"
                    android:gravity="center_vertical"
                    android:textStyle="bold"



                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
            </LinearLayout>
            <LinearLayout
                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/sr_no"
                    android:layout_width="100dp"

                    android:text=""
                    android:gravity="center_vertical"



                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:id="@+id/tv"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"



                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:id="@+id/tv1"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"



                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_marginTop="5dp"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout                 android:background="#FFE9E9"

                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="100dp"
                    android:text="Level"
                    android:gravity="center_vertical"
                    android:textStyle="bold"



                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="Join"
                    android:gravity="center_vertical"

                    android:textStyle="bold"


                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text="Sponser Id"
                    android:gravity="center_vertical"
                    android:textStyle="bold"



                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
            </LinearLayout>
            <LinearLayout
                android:padding="2dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/tv2"
                    android:layout_width="100dp"

                    android:text="66666666"
                    android:gravity="center_vertical"



                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"
                    android:id="@+id/tv3"
                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
                <TextView
                    android:id="@+id/tv4"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:text=""
                    android:gravity="center_vertical"



                    android:textColor="@color/black"

                    android:layout_height="match_parent">
                </TextView>
            </LinearLayout>

        </LinearLayout>



    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:background="@color/grey"
        android:layout_height="11dp">
    </View>



</LinearLayout>
